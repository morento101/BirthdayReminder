[flake8]
exclude =
    .git,
    .gitignore,
    __pycache__,
    venv,
    .env,
    .env.example,
    requirements.txt,
    README.md,
    __pycache__,
    .pytest_cache,
    .vscode,
    .idea,
    docker-compose.yml,
    Dockerfile,
    pytest.ini

ignore = F401,F841,F811

docstring-convention = google
